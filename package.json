{
	"name": "my-app1",
	"type": "module",
	"scripts": {
		"dev": "bun run scripts/dev.ts",
		"start": "bun run --hot src/index.ts",
		"tailwind": "bun run tailwindcss --input src/styles/main.css --output static/main.css --watch",
		"migrate:up": "bun run src/db/migrator.ts --up",
		"migrate:d": "bun run src/db/migrator.ts --down"
	},
	"dependencies": {
		"@conform-to/zod": "^1.3.0",
		"@node-rs/argon2": "^2.0.2",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@tailwindcss/cli": "^4.0.15",
		"dayjs": "^1.11.13",
		"hono": "^4.7.5",
		"kysely": "^0.27.6",
		"kysely-bun-sqlite": "^0.3.2",
		"tailwindcss": "^4.0.15",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"daisyui": "^5.0.9"
	},
	"trustedDependencies": ["@parcel/watcher", "bcrypt"]
}
